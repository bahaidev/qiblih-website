{
  "private": true,
  "name": "qiblih-website",
  "version": "0.1.0",
  "description": "Home for https://qiblih.com",
  "scripts": {
    "open-dev": "open-cli http://127.0.0.1:3000/src/ && npm start",
    "open": "open-cli http://127.0.0.1:3000/public/ && npm start",
    "start": "serve",
    "rollup": "rollup -c",
    "eslint": "eslint --report-unused-disable-directives .",
    "update-cdns": "integrity src/index.html --dropModules --dropBase --forceIntegrityChecks --urlIntegrityCheck -o public/index.html",
    "build": "npm run rollup && npm run update-cdns"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bahaidev/qiblih-website.git"
  },
  "keywords": [
    "bahai",
    "qiblih"
  ],
  "author": "William Cooley (https://www.wtip.net/)",
  "contributors": [
    "Brett Zamir <brettz9@yahoo.com> (http://brett-zamir.me/)"
  ],
  "license": "(MIT AND UNLICENSED)",
  "bugs": {
    "url": "https://github.com/bahaidev/qiblih-website/issues"
  },
  "homepage": "https://github.com/bahaidev/qiblih-website#readme",
  "engines": {},
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@fortawesome/fontawesome-free": "^5.15.2",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.2.1",
    "bootstrap": "^4.6.0",
    "eslint": "^8.30.0",
    "esri-leaflet": "^2.5.3",
    "esri-leaflet-geocoder": "^2.3.4",
    "geomag": "^1.0.0",
    "integrity-matters": "^0.14.0",
    "jquery": "^3.5.1",
    "jquery.easing": "^1.4.1",
    "leaflet": "^1.7.1",
    "leaflet.geodesic": "^2.6.1",
    "open-cli": "^7.1.0",
    "popper.js": "^1.16.1",
    "rollup": "^3.9.0",
    "rollup-plugin-copy": "^3.4.0",
    "serve": "^14.1.2"
  }
}
